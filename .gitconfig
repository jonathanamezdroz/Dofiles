[user]
	email = ychevallier@etel.ch
	name = Yves Chevallier
[code]
	editor = vim
[core]
	ignorecase = false
	autocrlf = true
	whitespace = cr-at-eol
[diff]
	tool = vimdiff
[alias]
    tree = log --graph --decorate --pretty=oneline --abbrev-commit
    co = checkout
    ci = commit
    st = status
    br = branch
